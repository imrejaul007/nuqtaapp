    <script>
        function showSheet(sheetName) {
            document.querySelectorAll('.sheet').forEach(sheet => {
                sheet.classList.remove('active');
            });
            document.querySelectorAll('.tab').forEach(tab => {
                tab.classList.remove('active');
            });
            document.getElementById(sheetName).classList.add('active');
            event.target.classList.add('active');
        }

        function updateAll() {
            // Get all inputs
            const aov = parseFloat(document.getElementById('aov').value) || 100;
            const frequency = parseFloat(document.getElementById('frequency').value) || 4;
            const cac = parseFloat(document.getElementById('cac').value) || 35;
            const monthlyRpu = parseFloat(document.getElementById('monthly-rpu').value) || 30;
            const retentionMonths = parseFloat(document.getElementById('retention-months').value) || 12;
            const activationRate = parseFloat(document.getElementById('activation-rate').value) || 30;
            const userChurn = parseFloat(document.getElementById('user-churn').value) || 8.3;
            const merchantChurn = parseFloat(document.getElementById('merchant-churn').value) || 5;
            const merchantFeeRate = parseFloat(document.getElementById('merchant-fee').value) || 15;
            const userCashbackRate = parseFloat(document.getElementById('user-cashback').value) || 7;
            const marketingPct = parseFloat(document.getElementById('marketing-pct').value) || 60;
            const techPct = parseFloat(document.getElementById('tech-pct').value) || 20;
            const opsPct = parseFloat(document.getElementById('ops-pct').value) || 20;
            const monthlyBurn = parseFloat(document.getElementById('monthly-burn-base').value) || 121000;
            const fixedCostPct = parseFloat(document.getElementById('fixed-cost-pct').value) || 40;
            const variableCostPct = parseFloat(document.getElementById('variable-cost-pct').value) || 60;
            const fundraiseUsd = parseFloat(document.getElementById('fundraise-usd').value) || 300000;
            const usdAedRate = parseFloat(document.getElementById('usd-aed-rate').value) || 3.67;
            const runwayTarget = parseFloat(document.getElementById('runway-months').value) || 9;

            // Calculate core metrics
            const netTakeRate = merchantFeeRate - userCashbackRate;
            const ltv = monthlyRpu * retentionMonths;
            const ltvCacRatio = ltv / cac;
            const gmvPerMau = aov * frequency;
            const cacPayback = cac / monthlyRpu;
            const grossMargin = 100; // Digital product
            const arpu = monthlyRpu;
            const contributionMargin = arpu; // No variable costs in revenue
            const cmPerUser = contributionMargin;

            const fundraiseAed = fundraiseUsd * usdAedRate;
            const fixedCost = monthlyBurn * (fixedCostPct / 100);
            const variableCost = monthlyBurn * (variableCostPct / 100);
            const breakEvenMau = Math.ceil(fixedCost / cmPerUser);

            const marketingSpend = monthlyBurn * (marketingPct / 100);
            const techSpend = monthlyBurn * (techPct / 100);
            const opsSpend = monthlyBurn * (opsPct / 100);

            // H1 calculations
            const mauByMonth = [300, 735, 1448, 2123, 2760];
            const cumulativeMau = [300, 1035, 2483, 4606, 7366];

            let cumulativeGmv = 0;
            let cumulativeRevenue = 0;
            let cashInHand = fundraiseAed;

            for (let i = 0; i < 5; i++) {
                const monthlyGmv = mauByMonth[i] * gmvPerMau;
                cumulativeGmv += monthlyGmv;
                const monthlyRevenue = monthlyGmv * (netTakeRate / 100);
                cumulativeRevenue += monthlyRevenue;
            }

            const month5Revenue = mauByMonth[4] * gmvPerMau * (netTakeRate / 100);
            const mrr = month5Revenue;
            const arr = mrr * 12;

            // Growth metrics
            const cmgr = Math.pow(mauByMonth[4] / mauByMonth[0], 1/4) - 1;
            const annualGrowth = (Math.pow(1 + cmgr, 12) - 1) * 100;
            const nrr = (1 - (userChurn / 100));

            // Burn metrics
            const netBurn = monthlyBurn - mrr;
            const burnMultiple = netBurn / (arr / 12);
            const magicNumber = (mrr * 12) / marketingSpend;
            const runwayRemaining = (fundraiseAed - (monthlyBurn * 5 - cumulativeRevenue)) / monthlyBurn;
            const cashRemaining = fundraiseAed - (monthlyBurn * 5 - cumulativeRevenue);

            // Update net take rate
            document.getElementById('net-take-rate').value = netTakeRate.toFixed(1);
            document.getElementById('fundraise-aed').value = Math.round(fundraiseAed);

            // Update dashboard
            document.getElementById('dash-ltvcac').textContent = ltvCacRatio.toFixed(1) + 'x';
            document.getElementById('dash-payback').textContent = cacPayback.toFixed(1);
            document.getElementById('dash-gross-margin').textContent = grossMargin + '%';
            document.getElementById('dash-burn-multiple').textContent = burnMultiple.toFixed(1);
            document.getElementById('dash-magic-number').textContent = magicNumber.toFixed(1);
            document.getElementById('dash-burn').textContent = 'AED ' + Math.round(monthlyBurn/1000) + 'K';
            document.getElementById('dash-mrr').textContent = 'AED ' + Math.round(mrr/1000) + 'K';
            document.getElementById('dash-arr').textContent = 'AED ' + Math.round(arr/1000) + 'K';
            document.getElementById('dash-arpu').textContent = 'AED ' + arpu;
            document.getElementById('dash-cm').textContent = 'AED ' + contributionMargin.toFixed(0);
            document.getElementById('dash-breakeven-mau').textContent = breakEvenMau.toLocaleString();
            document.getElementById('dash-gmv-mau').textContent = 'AED ' + gmvPerMau;
            document.getElementById('dash-cmgr').textContent = (cmgr * 100).toFixed(0) + '%';
            document.getElementById('dash-user-churn').textContent = userChurn + '%';
            document.getElementById('dash-merchant-churn').textContent = merchantChurn + '%';
            document.getElementById('dash-nrr').textContent = (nrr * 100).toFixed(0) + '%';

            // Update advanced metrics
            document.getElementById('adv-ltv').textContent = 'AED ' + ltv;
            document.getElementById('adv-cac').textContent = 'AED ' + cac;
            document.getElementById('adv-ltvcac').textContent = ltvCacRatio.toFixed(1) + 'x';
            document.getElementById('adv-cac-payback').textContent = cacPayback.toFixed(1);
            document.getElementById('adv-arpu').textContent = 'AED ' + arpu;
            document.getElementById('adv-cm').textContent = 'AED ' + contributionMargin.toFixed(0);
            document.getElementById('adv-cm-per-user').textContent = 'AED ' + cmPerUser.toFixed(0);
            document.getElementById('adv-gross-margin').textContent = grossMargin + '%';
            document.getElementById('adv-mrr').textContent = 'AED ' + Math.round(mrr/1000) + 'K';
            document.getElementById('adv-arr').textContent = 'AED ' + Math.round(arr/1000) + 'K';
            document.getElementById('adv-cmgr').textContent = (cmgr * 100).toFixed(0) + '%';
            document.getElementById('adv-annual-growth').textContent = annualGrowth.toFixed(0) + '%';
            document.getElementById('adv-gmv').textContent = 'AED ' + (cumulativeGmv/1000000).toFixed(2) + 'M';
            document.getElementById('adv-take-rate').textContent = netTakeRate + '%';
            document.getElementById('adv-nrr').textContent = (nrr * 100).toFixed(0) + '%';
            document.getElementById('adv-burn').textContent = 'AED ' + Math.round(monthlyBurn/1000) + 'K';
            document.getElementById('adv-burn-multiple').textContent = burnMultiple.toFixed(1);
            document.getElementById('adv-magic').textContent = magicNumber.toFixed(1);
            document.getElementById('adv-runway').textContent = runwayRemaining.toFixed(1);
            document.getElementById('adv-cash').textContent = 'AED ' + (fundraiseAed/1000000).toFixed(2) + 'M';
            document.getElementById('adv-breakeven').textContent = breakEvenMau.toLocaleString();
            document.getElementById('adv-breakeven-year').textContent = 'Year 4';
            document.getElementById('adv-months-breakeven').textContent = '36+';
            document.getElementById('adv-fixed-cost').textContent = 'AED ' + Math.round(fixedCost/1000) + 'K';
            document.getElementById('adv-variable-cost').textContent = 'AED ' + Math.round(variableCost/1000) + 'K';
            document.getElementById('adv-marketing').textContent = 'AED ' + Math.round(marketingSpend/1000) + 'K';
            document.getElementById('adv-tech').textContent = 'AED ' + Math.round(techSpend/1000) + 'K';
            document.getElementById('adv-ops').textContent = 'AED ' + Math.round(opsSpend/1000) + 'K';
            document.getElementById('adv-downloads').textContent = '10,000';
            document.getElementById('adv-activation').textContent = activationRate + '%';
            document.getElementById('adv-mau').textContent = cumulativeMau[4].toLocaleString();
            document.getElementById('adv-user-churn-display').textContent = userChurn + '%';
            document.getElementById('adv-merchants').textContent = '215';
            document.getElementById('adv-merchant-churn-display').textContent = merchantChurn + '%';
            document.getElementById('adv-users-per-merchant').textContent = Math.round(cumulativeMau[4] / 215);
            document.getElementById('adv-trans-merchant').textContent = Math.round((cumulativeMau[4] * frequency) / 215 / 30);

            // Update H1 monthly
            for (let i = 0; i < 5; i++) {
                const monthlyGmv = mauByMonth[i] * gmvPerMau;
                const monthlyRevenue = monthlyGmv * (netTakeRate / 100);
                document.getElementById(`h1-gmv-m${i+1}`).textContent = 'AED ' + (monthlyGmv/1000).toFixed(0) + 'K';
                document.getElementById(`h1-revenue-m${i+1}`).textContent = 'AED ' + (monthlyRevenue/1000).toFixed(0) + 'K';

                let cumGmv = 0;
                for (let j = 0; j <= i; j++) {
                    cumGmv += mauByMonth[j] * gmvPerMau;
                }
                document.getElementById(`h1-cum-gmv-m${i+1}`).textContent = 'AED ' + (cumGmv/1000000).toFixed(2) + 'M';

                const netBurnMonth = monthlyBurn - monthlyRevenue;
                document.getElementById(`h1-burn-m${i+1}`).textContent = '-AED ' + (netBurnMonth/1000).toFixed(0) + 'K';
            }

            document.getElementById('h1-revenue-total').textContent = 'AED ' + (cumulativeRevenue/1000).toFixed(0) + 'K';
            document.getElementById('h1-burn-total').textContent = '-AED ' + ((monthlyBurn * 5 - cumulativeRevenue)/1000).toFixed(0) + 'K';
            document.getElementById('h1-gap-gmv').textContent = 'AED ' + (cumulativeGmv/1000000).toFixed(2) + 'M';
            document.getElementById('h1-gap-multiple').textContent = (10000000 / cumulativeGmv).toFixed(1) + 'x boost';

            // Update income statement
            document.getElementById('income-gmv-h1').textContent = 'AED ' + (cumulativeGmv/1000000).toFixed(2) + 'M';
            document.getElementById('income-revenue-h1').textContent = 'AED ' + (cumulativeRevenue/1000).toFixed(0) + 'K';
            document.getElementById('income-ltvcac').textContent = ltvCacRatio.toFixed(1) + 'x';

            // Update cash flow
            let cash = fundraiseAed;
            for (let i = 0; i < 5; i++) {
                const monthlyRevenue = mauByMonth[i] * gmvPerMau * (netTakeRate / 100);
                const netBurnMonth = monthlyBurn - monthlyRevenue;

                document.getElementById(`cf-cash-m${i+1}-start`).textContent = 'AED ' + (cash/1000).toFixed(0) + 'K';
                document.getElementById(`cf-revenue-m${i+1}`).textContent = 'AED ' + (monthlyRevenue/1000).toFixed(0) + 'K';
                document.getElementById(`cf-burn-m${i+1}`).textContent = '-AED ' + (netBurnMonth/1000).toFixed(0) + 'K';
                cash -= netBurnMonth;
                document.getElementById(`cf-cash-m${i+1}-end`).textContent = 'AED ' + (cash/1000).toFixed(0) + 'K';
                document.getElementById(`cf-surplus-m${i+1}`).textContent = 'AED 0K';
            }

            document.getElementById('cf-marketing-fixed').textContent = 'AED ' + (marketingSpend/1000).toFixed(0) + 'K';
            document.getElementById('cf-marketing-variable').textContent = 'AED 0K';
            document.getElementById('cf-marketing-total').textContent = 'AED ' + (marketingSpend/1000).toFixed(0) + 'K';
            document.getElementById('cf-bd-fixed').textContent = 'AED 15K';
            document.getElementById('cf-bd-variable').textContent = 'AED 0K';
            document.getElementById('cf-bd-total').textContent = 'AED 15K';
            document.getElementById('cf-final-cash').textContent = 'AED ' + (cash/1000).toFixed(0) + 'K';
            document.getElementById('cf-runway').textContent = (cash / monthlyBurn).toFixed(1);
            document.getElementById('cf-cash-remaining').textContent = 'AED ' + (cash/1000).toFixed(0) + 'K';
            document.getElementById('cf-runway-display').textContent = (cash / monthlyBurn).toFixed(1) + ' months';

            document.getElementById('exp-marketing').textContent = 'AED ' + (marketingSpend/1000).toFixed(0) + 'K';
            document.getElementById('exp-tech').textContent = 'AED ' + (techSpend/1000).toFixed(0) + 'K';
            document.getElementById('exp-ops').textContent = 'AED ' + (opsSpend/1000).toFixed(0) + 'K';
            document.getElementById('exp-total').textContent = 'AED ' + (monthlyBurn/1000).toFixed(0) + 'K';
            document.getElementById('exp-marketing-h1').textContent = 'AED ' + (marketingSpend * 5 / 1000).toFixed(0) + 'K';
            document.getElementById('exp-tech-h1').textContent = 'AED ' + (techSpend * 5 / 1000).toFixed(0) + 'K';
            document.getElementById('exp-ops-h1').textContent = 'AED ' + (opsSpend * 5 / 1000).toFixed(0) + 'K';
            document.getElementById('exp-total-h1').textContent = 'AED ' + (monthlyBurn * 5 / 1000).toFixed(0) + 'K';

            // Update sensitivity
            document.getElementById('sens-ltv-base').textContent = 'AED ' + ltv;
            document.getElementById('sens-cac-base').textContent = 'AED ' + cac;
            document.getElementById('sens-ratio-base').textContent = ltvCacRatio.toFixed(1) + 'x';
            document.getElementById('sens-ltv-opt').textContent = 'AED ' + ltv;
            document.getElementById('sens-cac-base2').textContent = 'AED ' + cac;
            document.getElementById('sens-ltv-base2').textContent = 'AED ' + ltv;
            document.getElementById('sens-ratio-base2').textContent = ltvCacRatio.toFixed(1) + 'x';
            document.getElementById('sens-ltv-cons').textContent = 'AED ' + ltv;
            document.getElementById('sens-ltv-pess').textContent = 'AED ' + ltv;
        }

        window.onload = function() {
            updateAll();
        };
    </script>
</body>
